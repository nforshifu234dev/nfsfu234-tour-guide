{
  "name": "nfsfu234-tour-guide",
  "version": "0.2.10",
  "description": "A plug-and-play React component for creating customizable onboarding tours with tooltips, modals, and smooth user guidance. Built with Tailwind CSS and supports dark mode, Framer Motion animations, and full control over step logic.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:js": "tsup",
    "build:css": "postcss src/index.css -o dist/index.css",
    "build": "npm run build:js && npm run build:css",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests yet, skipping...\" && exit 0",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "react",
    "onboarding",
    "product-tour",
    "user-guidance",
    "react-component",
    "react-tour",
    "interactive-ui",
    "tooltips",
    "modals",
    "framer-motion",
    "lucide-react",
    "tailwindcss",
    "react-onboarding",
    "nextjs",
    "nforshifu234dev",
    "nforshifu234dev/tour-guide",
    "nfsfu234-tour-guide",
    "nfsfu234"
  ],
  "author": "NFORSHIFU234DEV",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nforshifu234dev/nfsfu234-tour-guide.git"
  },
  "bugs": {
    "url": "https://github.com/nforshifu234dev/nfsfu234-tour-guide/issues"
  },
  "homepage": "https://github.com/nforshifu234dev/nfsfu234-tour-guide#readme",
  "peerDependencies": {
    "framer-motion": "^12.12.1",
    "lucide-react": "^0.511.0",
    "react": ">=18.3.1 <20",
    "react-dom": ">=18.3.1 <20",
    "tailwindcss": "^4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^22.15.18",
    "@types/react": ">=18.3.1 <20",
    "@types/react-dom": ">=18.3.1 <20",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.7",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "release": {
    "commitUrlFormat": "https://github.com/nforshifu234dev/nfsfu234-tour-guide/commit/{{hash}}",
    "issueUrlFormat": "https://github.com/nforshifu234dev/nfsfu234-tour-guide/issues/{{id}}"
  }
}
